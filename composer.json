    },
    "autoload": {
        "psr-4": {
            "junaid-A-khan\\ExtendedResourceAuthorization\\": "src/"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "junaid-A-khan\\ExtendedResourceAuthorization\\ExtendedResourceAuthorizationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
