{
    "name": "junaid-ahmad-khan/laravel-extended-resource-authorization",
    "description": "Extended Resource Authorization",
    "homepage": "https://github.com/junaid-A-khan/laravel-extended-resource-authorization",
    "keywords":
    [
        "laravel",
        "extended",
        "resource",
        "policy",
        "authorization",
        "authorize custom methods"
    ],
    "authors": [
        {
            "name": "junaidahmadkhan",
            "email": "junaid851@gmail.com"
        }
    ],
    "minimum-stability": "dev",
